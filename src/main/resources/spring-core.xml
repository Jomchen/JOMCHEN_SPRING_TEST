<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- 导入了工具配置 -->
    <import resource="spring-util.xml"/>

    <!-- 自定义练习配置 -->
    <bean id="customer00" class="com.jomchen.springtest.entity.basedata.Customer">
        <constructor-arg index="0" type="java.lang.Integer" value="0"/>
        <constructor-arg index="1" type="java.lang.Integer" value="20"/>
        <constructor-arg index="2" type="java.lang.String" value="customer00"/>
        <constructor-arg index="3" type="java.lang.String" value="北京00"/>
        <constructor-arg index="4" type="java.util.Date">
            <null/>
        </constructor-arg>
    </bean>



    <!-- 学到 7.4.2 -->

</beans>